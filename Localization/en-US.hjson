			# ==========================================
			ApothecaryPotion: {
				DisplayName: Sequence 9: Apothecary Potion
				Tooltip:
					'''
					Advance to Apothecary.
					Grants poison resistance and increased life regeneration.
					Skilled in concocting potions and healing.
					'''
			}

			BeastTamerPotion: {
				DisplayName: Sequence 8: Beast Tamer Potion
				Tooltip:

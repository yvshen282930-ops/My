			# ==========================================
			#              Special Items
			# ==========================================
			BlasphemySlate: {
				DisplayName: Blasphemy Slate
				Tooltip:
					'''
					This ancient slate is inscribed with blasphemous text...
					It records lost potion formulas.
					[c/FFA500:【Consumable】Can be used as a material for any Sequence Potion]
					(The slate will shatter upon use)
					'''
			}

			PaperCard: {
				DisplayName: Tarot Card
				Tooltip:
					'''
					Sequence 8: Clown's common weapon.
					Seemingly ordinary cards, but sharp as blades when infused with spirituality.
					'''
			}

			ExtraordinaryBlood: {
				DisplayName: Extraordinary Blood
				Tooltip: Blood containing extraordinary power.
			}

			TestSummon: {
				DisplayName: Test Summon Staff
				Tooltip: Developer item for testing summon mechanics.
			}

			RoselleDiary: {
				DisplayName: Roselle's Diary
				Tooltip:
					'''
					"The taste of a Demoness isn't bad..."
					But this page seems to be about something else.
					<Left Click to read the beginner's guide>
					'''
			}

			UnshadowedCross: {
				DisplayName: Unshadowed Cross
				Tooltip:
					'''
					[c/FFD700:【Unique Artifact】]
					"Praise the Sun!"

					[c/FFA07A:Effect:]
					Use to precipitate Beyonder characteristics from your body.
					Lowers your Sequence level by 1 each use (e.g., Seq 1 -> Seq 2).
					Repeated use will eventually revert you to a mortal.
					This item contains eternal divinity and will not be consumed or destroyed.
					'''
			}

			PaperFigurine: {
				DisplayName: Paper Figurine
				Tooltip: A medium for the Sequence 4: Paper Figurine Substitute ability.
			}

			DogItem: {
				DisplayName: Dog Whistle
				Tooltip: Used to call your Extraordinary Dog.
			}

			ResurrectionReset: {
				DisplayName: Reset Resurrection (Test)
				Tooltip: Instantly resets all resurrection cooldowns.
			}

			RitualInstaComplete: {
				DisplayName: Instant Ritual (Test)
				Tooltip: Instantly completes the current potion ritual.
			}

			SpiritualityRefill: {
				DisplayName: Refill Spirituality (Test)
				Tooltip: Instantly refills spirituality.
			}

			# ==========================================
			#              Special Weapons
			# ==========================================
			# Giant Pathway
			TwilightGiantSword: {
				DisplayName: Twilight Giant Sword
				Tooltip:
					'''
					Sequence 2: Symbol of the Glory.
					Every strike ends with this, containing the authority of decay and twilight.
					Releases a twilight storm on attack, disintegrating enemy defense.
					'''
			}

			SwordOfDawn: {
				DisplayName: Sword of Dawn
				Tooltip:
					'''
					Sequence 6: Companion weapon of the Dawn Knight.
					Slashes release the brilliance of dawn, dealing devastating damage to undead.
					"Dawn arrives, darkness dissipates."
					'''
			}

			SilverRapier: {
				DisplayName: Silver Rapier
				Tooltip:
					'''
					Sequence 3: Rapier of the Silver Knight.
					The blade flows like mercury, attacking with extreme speed.
					'''
			}

			# Hunter Pathway
			ConquerorSword: {
				DisplayName: Iron-blooded Sword
				Tooltip:
					'''
					Sequence 4: Weapon of the Iron-blooded Knight.
					Forged in blood and fire, yearning for battle.
					Hits may ignite enemies.
					'''
			}

			ReaperScythe: {
				DisplayName: Reaper Scythe
				Tooltip:
					'''
					Sequence 5: Weapon of the Reaper.
					Specially designed to harvest the weaknesses of life, guaranteed critical hits.
					'''
			}

			ConquerorLance: {
				DisplayName: Conqueror's Lance
				Tooltip:
					'''
					Sequence 1: Weapon of the Conqueror.
					Indomitable, conquering all who disobey.
					'''
			}

			# Fool Pathway
			AirBullet: {
				DisplayName: Air Bullet
				Tooltip:
